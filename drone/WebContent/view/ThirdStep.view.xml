<core:View 
		 xmlns:core="sap.ui.core" 
		 xmlns:mvc="sap.ui.core.mvc" 
		 xmlns:viz="sap.viz.ui5.controls"
		 xmlns:l="sap.ui.layout"
  		 xmlns:m="sap.m"
  		 xmlns:commons="sap.ui.commons"
		controllerName="drone.view.ThirdStep" xmlns:html="http://www.w3.org/1999/xhtml">
	<m:Page title="{i18n>TITLE}" showNavButton="true" navButtonPress="toSecondStep">
		<m:content>
			<l:VerticalLayout>
				<l:HorizontalLayout>
					
					
					<l:VerticalLayout>
  						<l:HorizontalLayout>
  							<m:Label text="Plant Type:  "></m:Label>
  							<m:Text text="Something"></m:Text>
  						</l:HorizontalLayout>
  					</l:VerticalLayout>
					<l:VerticalLayout>
						<m:Image
							id="plantImageID"
							width="500px" height="500px">
							<!-- src="img/planta.jpg" -->
          					
          					<!-- <layoutData>
            					<FlexItemData growFactor="1" />
          					</layoutData> -->
          				</m:Image>
          			</l:VerticalLayout>
          			
					<l:VerticalLayout>
						<viz:Popover id="idPopOver"></viz:Popover>
		  				<viz:VizFrame id="idVizFrameLine" uiConfig="{applicationSet:'fiori'}" vizType="bar" width="500px" height="500px">
		  				</viz:VizFrame>	
  					</l:VerticalLayout>
  				</l:HorizontalLayout>
			</l:VerticalLayout>
		</m:content>
	</m:Page>
       
</core:View>